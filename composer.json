{
    "name": "treolabs/bolt3-twig-emailobfuscator",
    "description": "Bolt CMS 3 Email obfuscator filter for twig",
    "type": "bolt-extension",
    "keywords": ["email", "obfuscate", "obfuscator"],
    "require": {
        "bolt/bolt": "^3.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "TreoLabs GmbH",
            "homepage": "https://treolabs.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\TreoLabs\\EmailObfuscator\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\TreoLabs\\EmailObfuscator\\EmailObfuscatorExtension"
    }
}
